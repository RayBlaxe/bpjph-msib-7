{
  "name": "halal__landing_page",
  "version": "1.0.0",
  "private": true,
  "description": "Halal Landing Page",
  "author": "Ainul Yaqin",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby serve",
    "build": "cross-env NODE_ENV=development GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages && npm run favicon:set",
    "build:prod": "cross-env NODE_ENV=production GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages && npm run favicon:set",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "favicon:set": "yes | cp -r src/favicon/* public/static",
    "copy:html": "yes | cp -r public html"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "formik": "^2.2.9",
    "gatsby": "^4.6.1",
    "gatsby-plugin-create-client-paths": "^4.9.0",
    "gatsby-plugin-gatsby-cloud": "^4.7.0",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-image": "^2.6.0",
    "gatsby-plugin-postcss": "^5.6.0",
    "gatsby-plugin-schema-snapshot": "^3.2.0",
    "gatsby-plugin-sharp": "^4.6.0",
    "gatsby-plugin-zopfli": "^2.0.0",
    "gatsby-source-graphql": "^4.25.0",
    "gatsby-source-strapi": "^2.0.0",
    "gatsby-transformer-remark": "^5.6.0",
    "gatsby-transformer-sharp": "^4.6.0",
    "graphql-request": "^5.1.0",
    "intl": "^1.2.5",
    "postcss": "^8.4.6",
    "ptz-i18n": "^1.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.7.1",
    "react-images-viewer": "^1.7.1",
    "react-intl": "^6.2.8",
    "react-query": "^3.39.3",
    "react-slick": "^0.28.1",
    "react-slideshow-image": "^4.0.5",
    "react-type-animation": "^2.1.2",
    "slick-carousel": "^1.8.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "tailwindcss": "^3.0.18"
  }
}
